<!DOCTYPE html>
<html>

<head>
    <title>Remove Customer</title>
</head>

<body>
    <form>
        <a href="/logout">Logout</a><br> <br>
        <a href="/admin">Home</a>
        <table border="1">
            <tr>
                <td>Customer Name</td>
                <td>User Name</td>
                <td>Contact NO</td>
                <td>ACTION</td>
            </tr>
    
        <% for(var i=0; i < users.length; i++){ %>
    
            <tr>
                <td><%= users[i].name %></td>
                <td><%= users[i].username %></td>
                <td><%= users[i].contactno %></td>
                <td> 
                    <a href="/admin/removecustomer/<%= users[i].id %>">Delete</a>
                </td>
            </tr>
        <% } %>
    
        </table>
        
    </form>
</body>

</html>